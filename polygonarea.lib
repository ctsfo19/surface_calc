struct vertex{
    double x;
    double y;
};

int det(int a, int b, int c, int d) {
	return a * d - b * c;
}

double triangleArea(vertex a, vertex b, vertex c)
{
	double xAB = b.x - a.x;
	double yAB = b.y - a.y;
	double xAC = c.x - a.x;
	double yAC = c.y - a.y;

	return 0.5*fabs(det(xAB, yAB, xAC, yAC));
}